<Window x:Class="ROM_Info.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ROM_Info"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Button x:Name="makeXml" Content="Make mame.xml" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="217" Height="23" Click="makeXml_Click"/>
        <Button x:Name="makeCsv" Content="Make roms.csv from mame.xml" HorizontalAlignment="Left" Margin="10,38,0,0" VerticalAlignment="Top" Width="217" Click="makeCsv_Click"/>

    </Grid>
</Window>
